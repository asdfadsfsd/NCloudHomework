import java.util.Scanner;
public class Main{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		

		int N = sc.nextInt();
		sc.nextLine();
		
		if(N % 4 == 0) {
			for(int i = 0 ; i < N / 4 ; i++) {
				System.out.print("long" + " ");}
			System.out.println("int");
		}
		
	}


}
